import java.util.Vector;

import org.apache.xmlrpc.XmlRpcClient;

public class Client {
	public static void main(String[] args) throws Exception{
		XmlRpcClient client = new XmlRpcClient("http://localhost:8080");
		Vector<Float> params = new Vector<Float>();
		params.add(new Float(21.0f));
        Float result = (Float) client.execute("TemperatureConversion.celsiusToFahrenheit", params);
        System.out.println(result);
	}
}
